
//convert the string to float int double using parse* method from Integer/Float/Dobule classes.
public class Day2_6 {
	public static void main(String[] args) {
		String str_num="123";
		int num_integer= Integer.parseInt(str_num);
		
		String str_float="45.424f";
		float num_float=Float.parseFloat(str_float);
		
		String str_double="42.4342";
		double num_double=Double.parseDouble(str_double);
		
		System.out.println(num_integer+" "+num_float+" "+num_double);
		
	}
}
