
// Variables demo


//N1 = 30  
// N2 = 25
public class Day2_4 {

	public static void main(String[] args) 
	{
		int n1=30;
		int n2=25;
		System.out.println("N1 = "+n1); // java
		// + is used for concatination
		
		//printf("N1 = %d",n1); //C 
		//cout<<"N1 = "<<n1; // C++ 
		
		
		System.out.println("N2 = "+n2); // java
		
		// Output : Num1 = 30  and Num2 = 25
		System.out.println("Num1 = "+n1+" and "+" Num2 = "+n2);
		
	}

}



/*
