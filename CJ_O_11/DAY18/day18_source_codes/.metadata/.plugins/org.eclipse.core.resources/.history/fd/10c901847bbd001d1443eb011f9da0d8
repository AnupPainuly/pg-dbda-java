class EvenNumbers extends Thread
{
	public void run()
	{
		for(int i=0;i<20;i+=2)
		{
			System.out.println("Even Number = "+i);
			try 
			{
				Thread.sleep(1000);
			} 
			catch (InterruptedException e) 
			{
				e.printStackTrace();
			}
		}
	}
}
public class Day18_7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
