package p1;

import java.io.File;

public class Day17_6
{
	static EmpPojo parseEmp(String line)
	{
		EmpPojo e = new EmpPojo();
		String[] parts = line.split(",");
		e.setId(Integer.parseInt(parts[0]));
		
		return e;
	}
	
	public static void main(String[] args) 
	{
		File f=new File(args[0]);
	}

}
