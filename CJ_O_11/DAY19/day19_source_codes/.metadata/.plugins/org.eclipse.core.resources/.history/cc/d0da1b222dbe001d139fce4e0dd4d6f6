
class Demo implements Runnable
{
	Thread t;
	String str;
	Demo(String str)
	{
		this.str=str;
	}
	
	@Override
	public void run()
	{
		System.out.println("Thread name = "+str);
		for(int i=0;i<5;i++)
			System.out.println(i);
	}
	
}

public class Day19_1 {

	public static void main(String[] args) 
	{
		
	}

}
