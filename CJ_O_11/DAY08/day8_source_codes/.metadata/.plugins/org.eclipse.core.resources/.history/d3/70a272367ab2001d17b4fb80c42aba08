class Machine
{
	public void start()
	{
		System.out.println("Machine started");
	}
}

class Laptop extends Machine
{
	public void start()
	{
		System.out.println("Laptop Started");
	}
	public void stop()
	{
		System.out.println("Laptop stopped");
	}
}
public class Day8_3 {

	public static void main(String[] args)
	{
		

	}

}
