import java.io.File;
import java.io.IOException;

//create a folder (directory)

public class Day13_7 {

	public static void main(String[] args) throws IOException 
	{
		String name = "MyDirectory";
		File fobj=new File(name);
		boolean status=fobj.mkdir();
		
	}
	
}


