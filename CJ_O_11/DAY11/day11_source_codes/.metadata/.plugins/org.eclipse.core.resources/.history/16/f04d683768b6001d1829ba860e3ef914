	public static void main(String[] args) 
	{
		ArrayList<Integer> intList=new ArrayList<Integer>();
		ArrayList<Double> doubleList=new ArrayList<Double>();
		ArrayList<String> strList=new ArrayList<String>();
		
		intList = getIntList();
