	getStrList()
	{
		
	}
	
	
	
	public static void printIntList(ArrayList<Integer> list) // function having argument as AL 
	{
		for(Integer ele : list)
			System.out.print(ele+"\t");
	}
	
	printDoubleList(doubleList)
