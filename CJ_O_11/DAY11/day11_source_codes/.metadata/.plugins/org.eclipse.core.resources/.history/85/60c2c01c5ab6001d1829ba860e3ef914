import java.util.ArrayList;

public class Day11_1 {

	public static void main(String[] args) 
	{
		ArrayList al=new ArrayList(); // al is of RAW TYPE
		// not recommended
		al.add(10);
		al.add(20);
		al.add(30);
		al.add(40);
		al.add(50);
		al.add("Sunbeam");
		al.add(5.5f);
		al.add(6.7);
		
	}
}


