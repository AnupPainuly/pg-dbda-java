
class Circle
{
	final float PI = 3.14f; // final / constant field 
	//final fields must be initialized at the time of declaration compulsory
	//final float PI; // Javac error 
	
	int radius; // non constant field 
	
		
	void print()
	{
		System.out.println("PI = "+PI);
	}
	
}

public class Day4_8 {

	public static void main(String[] args) 
	{
		Circle cobj = new Circle();
		cobj.print();

	}

}
