class Employee
{
	String name;
	int empid;
	float salary;
	String designation;
	String dept;
	
	public Employee() 
	{
		this("DEFAULT",1,0.0f,"DEFAULT","DEFAULT");
	}

	public Employee(String name, int empid, float salary, String designation, String dept) 
	{
		
		this.name = name;
		this.empid = empid;
		this.salary = salary;
		this.designation = designation;
		this.dept = dept;
	}
	
	
	
	
	
}

