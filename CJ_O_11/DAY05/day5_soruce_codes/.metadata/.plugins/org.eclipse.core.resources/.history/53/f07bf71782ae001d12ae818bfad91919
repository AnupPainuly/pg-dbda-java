


public class Day5_4 {

	public static void main(String[] args) 
	{
		System.out.println("PI Value = "+PI);
		System.out.println("Max value = "+max(45, 25));
		System.out.println("Min value = "+min(23, 49));
		System.out.println("Power = "+pow(8, 3));
		System.out.println("SQRT = "+sqrt(25.0));

	}

}


/*
