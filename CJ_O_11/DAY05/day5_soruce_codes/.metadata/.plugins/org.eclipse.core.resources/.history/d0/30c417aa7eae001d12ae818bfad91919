package p1;

import java.util.Scanner;

public class Day5_3 
{
	static int num1;
	static int num2;
	
	static
	{
		num1=5;
		num2=5;
	}
	
	static void accept()
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter num1 ");
		num1=sc.nextInt();
		System.out.println("Enter num2 ");
		num2=sc.nextInt();
		sc.close();
	}
	static void disp()
	{
		System.out.println("Num1 = "+num1+" Num2 = "+num2);
	}

	public static void main(String[] args) 
	{
		disp();
		accept();
		disp();
		
	}

}
