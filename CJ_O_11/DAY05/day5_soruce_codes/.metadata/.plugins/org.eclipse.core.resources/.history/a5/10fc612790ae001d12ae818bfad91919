import java.util.Arrays;

public class Day5_10 {

	public static void main(String[] args)
	{
		int a[]=new int[] {500,120,130,240,150};
		
		System.out.println("Printing array using normal for loop");
		for(int i=0;i<5;i++)
			System.out.print(a[i]+"\t");
		
		System.out.println();
		
		System.out.println("Printing array using advanced for loop");
		for(int ele : a)
			System.out.print(ele +"\t");
		
		System.out.println();
		
		System.out.println("Printing each element individually");
		System.out.print(a[0] +"\t");
		System.out.print(a[1] +"\t");
		System.out.print(a[2] +"\t");
		System.out.print(a[3] +"\t");
		System.out.print(a[4] +"\t");
		System.out.println();
		
		System.out.println("Print the elements using toString() from Arrays class");
		
		System.out.println(Arrays.toString(a));
		
	}
}


