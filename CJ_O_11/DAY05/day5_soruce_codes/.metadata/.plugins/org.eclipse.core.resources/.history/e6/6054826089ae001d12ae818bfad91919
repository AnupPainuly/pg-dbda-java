import java.util.Scanner;



public class Day5_6 {

	public static void main(String[] args)
	{
		int arr[]=new int[4];
		float farr[]=new float[4];
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Array elements (int type):");
		for(int i=0;i<4;i++)
			arr[i]=sc.nextInt();
	
	
	}
	
}


