
class Person
{
	String name;
	int age;
	
	Person()
	{
		System.out.println("Inside Parameterless Const Person");
	}
	
	void disp()
	{
		System.out.println("Name = "+this.name+ " Age = "+this.age);
	}
}

class Employee extends Person
{
	int empid;
	int salary;
	Employee()
	{
		
	}
}


