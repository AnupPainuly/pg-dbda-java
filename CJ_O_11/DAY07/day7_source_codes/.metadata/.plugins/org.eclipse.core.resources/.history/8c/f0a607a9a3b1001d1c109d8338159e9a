
public class Day7_4 {

	public static void main(String[] args)
	{
		String s1="Akshita";
		String s2="Sunbeam";
		System.out.println("char at 2nd location "+s1.charAt(2));
		System.out.println("s1 compare to s2 "+s1.compareTo(s2));
		// ASCII VALUES
		// A 65  S 83   -18  
		String s3=s1+"Chanchlani";
		System.out.println("S3 = "+s3);
		
		
	}

}
