class Product
{
	int pid;

	public Product() 
	{
		super();
		
	}

	public Product(int pid) 
	{
		super();
		this.pid = pid;
	}

	@Override
	public String toString() {
		return "Product [pid=" + pid + "]";
	}
	
	
}
class Mobile extends Product
{
	int price;
	String mname;
	
}


